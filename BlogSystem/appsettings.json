{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "SassCompiler": {
    "Source": "Styles",
    "Target": "wwwroot/css",
    "Compilations": [
      {
        "Source": "wwwroot/assets/scss/style.scss",
        "Target": "wwwroot/assets/css/style.css"
      }
    ]
  }
}